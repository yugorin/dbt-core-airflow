[sqlfluff]
dialect = snowflake
templater = raw
max_line_length = 80
exclude_rules = L042

[sqlfluff:layout:type:comma]
spacing_after = single
line_position = leading

[sqlfluff:rules:aliasing.table]
aliasing = explicit

[sqlfluff:rules:aliasing.column]
aliasing = explicit

[sqlfluff:rules:references.consistent]
force_enable = true